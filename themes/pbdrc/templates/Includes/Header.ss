<!-- HEADER --><div class="container-fluid">    <header class="header">        <div class="container">            <div class="row">                <!-- PBDRC Header Logo -->                <div class="col-md-5">                <a class="brand" href="$AbsoluteBaseURL" style="">                    $SiteConfig.LogoImage                </a>                </div>                <!-- Header Rightside Sponsors -->                <div class="col-md-7 media sponsor-media">                    <div class="media-body sponsor-body">                        <h4 class="text-right">Brought to you by </h4>                    </div>                    <% control $GetHeader() %>                    <% if $Active %>                        <a class="header-sponsor" href="$AdvertURL" target="_blank">                            <img src="$AdvertImage.URL" class="advert-imgs img-fluid" alt="$AdvertAlt">                        </a>                    <% end_if %>                    <% end_control %>                </div>            </div>        </div>    </header>